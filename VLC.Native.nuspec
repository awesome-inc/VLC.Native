<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>VLC.Native</id>
        <version>$Version$.$Build$</version>
        <authors>Awesome Incremented and Contributors</authors>
        <owners>http://www.videolan.org/</owners>
        <licenseUrl>http://www.gnu.org/licenses/gpl-2.0.txt</licenseUrl>
        <projectUrl>https://github.com/awesome-inc/VLC.Native</projectUrl>
        <iconUrl>http://images.videolan.org/images/favicon.ico</iconUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <description>VLC native dependencies.</description>
        <releaseNotes>VLC v$Version$ from
- x86: http://download.videolan.org/pub/videolan/vlc/last/win32/vlc-$Version$-win32.zip
- x64: http://download.videolan.org/pub/videolan/vlc/last/win64/vlc-$Version$-win64.zip</releaseNotes>
        <copyright>Copyright © 1996-2015 VideoLAN and VLC Authors</copyright>
        <tags>vlc</tags>
    </metadata>
    <files>
        <file src="content\*.*" target="content" />
        <file src="build\*.*" target="build" />
        <file src="dist\x86\vlc-$Version$\**\*.*" target="vlc\x86" />
        <file src="dist\x64\vlc-$Version$\**\*.*" target="vlc\x64" />
    </files>
</package>